{
  "name": "polygons",
  "version": "0.0.2",
  "description": "Draw polygons using a ruler and straight edge",
  "main": "index.js",
  "engines": {
    "node": "=8"
  },
  "scripts": {
    "postinstall": "NODE_ENV=production ./node_modules/.bin/webpack",
    "start": "node index.js",
    "dev": "NODE_ENV=development node index.js",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --require babel-core/register \"test/**/*.spec.js\"",
    "lint": "NODE_ENV=test ./node_modules/.bin/eslint app src test webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felamaslen/polygons.git"
  },
  "author": "Fela Maslen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/felamaslen/polygons/issues"
  },
  "homepage": "https://github.com/felamaslen/polygons#readme",
  "babel": {
    "env": {
      "production": {
        "presets": [
          "react",
          [
            "env",
            {
              "targets": {
                "browsers": [
                  "last 2 versions",
                  "ie >= 10"
                ]
              }
            }
          ],
          "stage-0"
        ],
        "sourceMaps": "inline"
      },
      "development": {
        "presets": [
          "react",
          "stage-0"
        ],
        "plugins": [],
        "sourceMaps": "inline"
      },
      "test": {
        "presets": [
          "react",
          [
            "env",
            {
              "targets": {
                "node": "8"
              }
            }
          ],
          "stage-0"
        ]
      }
    }
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "dotenv": "^5.0.0",
    "dotenv-webpack": "^1.5.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-plugin-react": "^7.6.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.6.2",
    "mocha": "^5.0.0",
    "redux-logger": "^3.0.6",
    "redux-test-utils": "^0.2.2",
    "webpack-dev-middleware": "^2.0.5",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.9",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "import-glob-loader": "^1.1.0",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-create-reducer": "^1.1.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "webpack": "^3.11.0",
    "webpack-spritesmith": "^0.4.0"
  }
}
